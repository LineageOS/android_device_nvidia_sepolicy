# Proc nodes
genfscon proc  /bus/pci/devices                                                            u:object_r:proc_pci_map:s0

# Sysfs nodes
genfscon sysfs /firmware/devicetree/base/chosen/plugin-manager/cvm                         u:object_r:sysfs_board:s0
genfscon sysfs /firmware/devicetree/base/chosen/proc-board/id                              u:object_r:sysfs_board:s0
genfscon sysfs /firmware/devicetree/base/chosen/proc-board/name                            u:object_r:sysfs_board:s0
genfscon sysfs /firmware/devicetree/base/chosen/proc-board/sku                             u:object_r:sysfs_board:s0
