# Proc nodes
genfscon proc /bus/pci/devices                                                            u:object_r:proc_pci:s0
genfscon proc /sys/kernel/printk                                                          u:object_r:proc_kernel:s0
genfscon proc /sys/vm/legacy_va_layout                                                    u:object_r:proc_legacy_va_layout:s0
genfscon proc /sys/vm/min_free_kbytes                                                     u:object_r:proc_min_free_kbytes:s0

# Sysfs nodes
genfscon sysfs /devices/7000c400.i2c/i2c-1/1-0040/driver                                   u:object_r:sysfs_nvi2c:s0
genfscon sysfs /devices/hda_pcm_map                                                        u:object_r:sysfs_audio:s0
genfscon sysfs /kernel/rt8168_power/mode                                                   u:object_r:sysfs_pwm_fan:s0
genfscon sysfs /firmware/devicetree/base/chosen/plugin-manager/cvm                         u:object_r:sysfs_board:s0
genfscon sysfs /firmware/devicetree/base/chosen/proc-board/id                              u:object_r:sysfs_board:s0
genfscon sysfs /firmware/devicetree/base/chosen/proc-board/name                            u:object_r:sysfs_board:s0
genfscon sysfs /firmware/devicetree/base/chosen/proc-board/sku                             u:object_r:sysfs_board:s0
